<%= @retrospective.title %>

Sections:

<% @retrospective.sections.each do |section| %>
    <%= "  #{section.title}:" %>
    <% section.items.each do |item| %>
        <%= "    * (#{item.item_votes.size}) #{item.value}" %>
    <% end %>
<% end %>

Groups:

<% @retrospective.groups.each do |group| %>
    <%= "  #{group.title}:" %>
    <% group.items.each do |item| %>
        <%= "    * #{item.value}" %>
    <% end %>
<% end %>
